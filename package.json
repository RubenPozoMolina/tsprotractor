{
  "name": "tsprotractor",
  "version": "1.0.0",
  "description": "Testing protractor with typescript",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RubenPozoMolina/tsprotractor.git"
  },
  "scripts": {
    "debug": "node --inspect-brk ./node_modules/.bin/protractor conf.js",
    "pretest": "npm run tsc && npm run webdriver-update",
    "test": "protractor conf.js",
    "tsc": "tsc",
    "webdriver-update": "webdriver-manager update --standalone=false --gecko=false"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "protractor": "^5.4.2",
    "ts-node": "7.0.1"
  },
  "devDependencies": {
    "@types/mocha": "5.2.6",
    "@types/node": "^10.5.6",
    "husky": "0.14.3",
    "mocha": "6.0.2",
    "tslint": "5.12.1",
    "typescript": "3.3.3",
    "@types/selenium-webdriver": "^4.0.5",
    "ts-mocha": "^6.0.0"
  },
