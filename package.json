    "tsc": "tsc",
    "webdriver-update": "webdriver-manager update --standalone=false --gecko=false"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "ts-node": "8.10.1"
  },
  "devDependencies": {
    "@types/mocha": "7.0.2",
    "@types/node": "^14.0.5",
    "@types/selenium-webdriver": "^4.0.9",
    "husky": "4.2.5",
    "mocha": "10.2.0",
    "protractor": "^7.0.0",
    "ts-mocha": "^7.0.0",
    "tslint": "6.1.2",
    "typescript": "3.9.3"
  },
