{
  "name": "tsprotractor",
  "version": "1.0.0",
  "description": "Testing protractor with typescript",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RubenPozoMolina/tsprotractor.git"
  },
  "scripts": {
    "debug": "node --inspect-brk ./node_modules/.bin/protractor conf.js",
    "pretest": "npm run tsc && npm run webdriver-update",
